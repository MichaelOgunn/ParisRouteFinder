<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_CENTER" prefHeight="700.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.parisroutefinder.MainController">
   <MenuBar prefHeight="17.0" prefWidth="900.0">
     <menus>
       <Menu mnemonicParsing="false" text="File">
         <items>
           <MenuItem mnemonicParsing="false" text="Open" />
             <MenuItem mnemonicParsing="false" text="Exit" />

         </items>
       </Menu>
       <Menu mnemonicParsing="false" text="Help">
         <items>
               <MenuItem mnemonicParsing="false" text="Clear" />
         </items>
       </Menu>
     </menus>
      <VBox.margin>
         <Insets bottom="20.0" />
      </VBox.margin>
   </MenuBar>
   <HBox alignment="CENTER" prefHeight="403.0" prefWidth="900.0">
      <children>
         <VBox alignment="CENTER" prefHeight="403.0" prefWidth="79.0">
            <children>
               <RadioButton mnemonicParsing="false" prefHeight="18.0" prefWidth="86.0" text="Waypoint" />
            </children>
         </VBox>
         <VBox alignment="CENTER" prefHeight="400.0" prefWidth="350.0">
            <children>
               <ImageView fx:id="mapImageView" fitHeight="400.0" fitWidth="350.0" onMouseClicked="#clickOnImage" onMouseMoved="#removeToolTip" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="@paris-map.png" />
                  </image></ImageView>
            </children>
         </VBox>
         <VBox alignment="CENTER" prefHeight="400.0" prefWidth="350.0">
            <children>
               <ImageView fx:id="bfsView" fitHeight="400.0" fitWidth="350.0" pickOnBounds="true" preserveRatio="true" />
            </children>
         </VBox>
      </children></HBox>
   <HBox alignment="CENTER" layoutX="10.0" layoutY="509.0" prefHeight="87.0" prefWidth="900.0">
      <children>
         <Button mnemonicParsing="false" prefHeight="66.0" prefWidth="154.0" text="Shortest Path(Dijkstra)" />
          <Button mnemonicParsing="false" prefHeight="66.0" prefWidth="154.0" text="Shortest Path(BFS)" />
         <Button layoutX="460.0" layoutY="90.0" mnemonicParsing="false" prefHeight="66.0" prefWidth="154.0" text="Most Historical Route" />
      </children>
   </HBox>
    <Label alignment="CENTER" prefHeight="6.0" prefWidth="530.0" text="Places to Avoid" textAlignment="CENTER">
        <font>
            <Font size="24.0" />
        </font>
    </Label>
   <HBox alignment="CENTER" layoutX="10.0" layoutY="546.0" prefHeight="46.0" prefWidth="900.0">
      <children>
         <CheckBox mnemonicParsing="false" prefHeight="18.0" prefWidth="106.0" text="Placeholder 1" />
         <CheckBox layoutX="413.0" layoutY="45.0" mnemonicParsing="false" prefHeight="18.0" prefWidth="106.0" text="Placeholder 2" />
         <CheckBox layoutX="460.0" layoutY="45.0" mnemonicParsing="false" prefHeight="18.0" prefWidth="106.0" text="Placeholder 3" />
      </children>
   </HBox>
</VBox>
